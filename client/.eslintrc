{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "no-console": 0
    },
    "env": {
            "node": true,
            "browser": true
        },
    "plugins": ["react"], 
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true 
        }
    }
}